        "type": "git",
        "url": "https://github.com/ChatLunaLab/storage-service.git",
        "directory": "."
    },
    "bugs": {
        "url": "https://github.com/ChatLunaLab/storage-service/issues"
    },
    "homepage": "https://github.com/ChatLunaLab/storage-service#readme",
